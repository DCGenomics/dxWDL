{
  "name": "native_diff",
  "summary": "Compare two files",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "a",
      "class": "file",
      "help": "Lefthand file to compare",
      "patterns": ["*.txt", "*.tsv"]
    },
    {
      "name": "b",
      "class": "file",
      "help": "Righthand file to compare",
      "patterns": ["*.txt", "*.tsv"]
    }
  ],
  "outputSpec": [
    {
      "name": "equality",
      "class": "boolean"
    }
  ],
  "runSpec": {
    "interpreter": "bash",
    "file": "code.sh",
    "distribution": "Ubuntu",
    "release": "16.04"
  },
  "authorizedUsers": []
}
